{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/prefer-stateless-function": "off",
        "react/prop-types": "off",
        "react/destructuring-assignment": "off",
        "class-methods-use-this": "off",
        "react/no-unused-class-component-methods": "off"
    }
}